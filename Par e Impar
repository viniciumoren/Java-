
import java.util.Scanner;

/**
 *
 * @author Aluno
 */
public class Main {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in); 
    
        int N = sc.nextInt();
        
       if (N<0) {
           System.out.println("Esse número é Negativo");
       }
       else {
           System.out.println("Numero é Positivo");
       }
       
       sc.close();
    }
}
