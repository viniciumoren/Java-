import java.util.Scanner;

/**
 *
 * @author Aluno
 */
public class Main {

    public static void main(String[] args) {
        Scanner sc = new Scanner (System.in) ;
                
        int N = sc.nextInt() ;
                
        if (N % 2 == 0) {
            System.out.println("O numero é par");         
        }
        else {
            System.out.println("O numero é impar");
        }
    }
}
